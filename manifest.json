{
    "name": "Digicampus dark mode",
    "description": "Digicampus angenehmer machen",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "tabs", "activeTab", "scripting"],
    "host_permissions": ["https://digicampus.uni-augsburg.de/"],
    "action": {
        "default_popup": "popup.html"
    }
}